<?xml version="1.0"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">

	<navigation-rule>
		<from-view-id>/vistas/iniciarSesion.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>irAMenuAdministrador</from-outcome>
			<to-view-id>/vistas/administrador/menuAdministrador.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>irAMenuOperador</from-outcome>
			<to-view-id>/vistas/operador/menuOperador.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
</faces-config>
